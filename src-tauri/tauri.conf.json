{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "roli-trade-ad-automation",
  "version": "0.1.0",
  "identifier": "com.admin.roli-trade-ad-automation",
  "build": {
  "beforeDevCommand": "npm run dev:frontend",
  "beforeBuildCommand": "npm run build:frontend",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "roli-trade-ad-automation",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
